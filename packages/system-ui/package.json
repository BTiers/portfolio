{
  "author": "BTiers",
  "name": "system-ui",
  "version": "1.0.0",
  "description": "System components",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles/global.css": "./dist/styles/global.css"
  },
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react && tailwindcss -i ./src/styles/global.css -o ./dist/styles/global.css",
    "dev": "concurrently \"tsup src/index.tsx --format esm,cjs --dts --external react --watch\" \"tailwindcss -i ./src/styles/global.css -o ./dist/styles/global.css --watch\"",
    "clean": "rm -rf dist"
  },
  "license": "MIT",
  "dependencies": {
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/modifiers": "^6.0.0",
    "immer": "^9.0.15",
    "lodash": "^4.17.21",
    "nanoid": "3.3.4",
    "react-icons": "^4.4.0",
    "react-resizable": "^3.0.4",
    "react-use": "^17.4.0",
    "tsup": "^6.2.3",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-resizable": "^3.0.3",
    "concurrently": "^7.4.0",
    "eslint": "^7.32.0",
    "eslint-config": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-config": "*",
    "ts-config": "*"
  }
}
